@model List<Data.EF.Category>

<div class="single-widget category">
    <h1 class="title text-center text-danger"><a href="#">Danh mục</a></h1>
    <ul class="categor-list">
        <li><a class="findByFK" name=0 href="#">Tất cả</a></li>
        @foreach (var item in Model)
        {
            <li><a class="findByFK" name=@item.id href="#">@item.name</a></li>
        }

    </ul>
</div>